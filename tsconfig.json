{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.js", "**/*.ts", "**/*.tsx", "services/index.js", "pages/category/[slug].js", "components/Author.jsx", "components/CommentForm.jsx", "components/Comments.jsx", "components/FeaturedPostCard.jsx", "components/Header.jsx", "components/index.jsx", "components/Layout.jsx", "components/Loader.jsx", "components/PostDetail.jsx", "pages/api/comments.js", "pages/post/[slug].jsx", "pages/index.jsx", "sections/FeaturedPosts.jsx", "pages/_app.jsx"],
  "exclude": ["node_modules"]
}
